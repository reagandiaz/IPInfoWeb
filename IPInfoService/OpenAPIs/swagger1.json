{
  "openapi": "3.0.1",
  "info": {
    "title": "PingService",
    "version": "v1"
  },
  "paths": {
    "/Worker": {
      "get": {
        "tags": [
          "Worker"
        ],
        "parameters": [
          {
            "name": "ip",
            "in": "query",
            "schema": {
              "type": "string",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Message": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "nullable": true
          },
          "roundtrip": {
            "type": "integer",
            "format": "int64"
          },
          "ttl": {
            "type": "integer",
            "format": "int64"
          },
          "dontfragment": {
            "type": "boolean"
          },
          "buffersize": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "Result": {
        "type": "object",
        "properties": {
          "ip": {
            "type": "string",
            "nullable": true
          },
          "message": {
            "$ref": "#/components/schemas/Message"
          },
          "state": {
            "type": "string",
            "nullable": true
          },
          "info": {
            "type": "string",
            "nullable": true
          },
          "ts": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    }
  }
}